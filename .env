# Google Gemini API Key (Required)
GOOGLE_API_KEY=

# GitHub Personal Access Token (Required for cloning/pushing)
GITHUB_TOKEN=

# MongoDB Atlas URI (Required for state persistence)
# Example: mongodb+srv://<user>:<password>@cluster.mongodb.net/?retryWrites=true&w=majority
MONGODB_URI=

# Optional: OpenRouter API Key (if using OpenRouter instead of Google directly)
# OPENROUTER_API_KEY=
